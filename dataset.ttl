ncp:NCG08089  a       nct:City ;
        rdfs:seeAlso  <http://viaf.org/viaf/sourceID/LC%7Cn++82070864#skos:Concept> , <http://id.worldcat.org/fast/1348263> , <http://www.wikidata.org/entity/Q2029146> , <http://id.worldcat.org/fast/1220047> , <http://id.loc.gov/authorities/names/n82070864> ;
